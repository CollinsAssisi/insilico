set title "HH Neuronal Network"
set xlabel 'Time Steps'
set ylabel 'Voltage'

if (!exists("infile")) infile='test_output.dat'
if (!exists("outfile")) outfile='test_output.jpeg'

set output outfile

data_title_1 = "Neuron 1"
data_title_2 = "Neuron 2"

set datafile separator ","
set term jpeg size 2048,1536 enhanced font 'Helvetica,10'
# set term wxt

set key inside right top vertical Right noreverse enhanced autotitles box linetype -1 linewidth 0.5
# set grid
# set border linewidth 1

plot infile using 1:2 with lines linestyle 1 linewidth 1 title data_title_1, infile using 1:3 with lines linestyle 2 linewidth 1 title data_title_2
